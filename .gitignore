anchor_stock_react/.pnp.js

# testing
anchor_stock_react/coverage

# misc
anchor_stock_react/.DS_Store
anchor_stock_react/.env.local
anchor_stock_react/.env.development.local
anchor_stock_react/.env.test.local
anchor_stock_react/.env.production.local

anchor_stock_react/npm-debug.log*
